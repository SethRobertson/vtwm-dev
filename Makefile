all:: vtwm

# To omit platform and build info on the Version window
#
menus.o:	menus.c
	 $(RM) $@ && \
	$(CC) -c $(CFLAGS) '-DNO_BUILD_INFO' $*.c

parse.o:	parse.c
	 $(RM) $@ && \
	$(CC) -c $(CFLAGS) '-DSYSTEM_INIT_FILE="'$(VTWMDIR)'/system.vtwmrc"' $*.c

depend:: lex.c gram.c deftwmrc.c lastmake.c

        PROGRAM = vtwm

all:: vtwm

vtwm: $(OBJS) $(DEPLIBS)
	  $(RM) $@
	 $(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: vtwm
	 -@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  vtwm $(DESTDIR)$(BINDIR)/vtwm

vtwm.nr.z : vtwm.man
	@if [ -f vtwm.man ]; then \
		$(RM) vtwm.nr vtwm.nr.z && \
		tbl vtwm.man | eqn | nroff -man - > vtwm.nr && \
		pack -f vtwm.nr; \
	fi

install.man:: vtwm.nr.z
	 -@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi; \
	if [ -f vtwm.man ]; then \
		$(INSTALL) -c $(INSTMANFLAGS) \
			vtwm.nr.z $(DESTDIR)$(MANDIR)/vtwm.z; \
	fi

clean::
	$(RM) vtwm.nr vtwm.nr.z

depend::
	 $(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	 $(RM) vtwm

install:: system.vtwmrc
	 -@if [ -d $(DESTDIR)$(VTWMDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(VTWMDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) system.vtwmrc $(DESTDIR)$(VTWMDIR)/system.vtwmrc

gram.h gram.c: gram.y
	yacc $(YFLAGS) gram.y
	$(MV) y.tab.c gram.c
	$(MV) y.tab.h gram.h

clean::
	$(RM) y.tab.h y.tab.c lex.yy.c gram.h gram.c lex.c deftwmrc.c lastmake.c

deftwmrc.c:  system.vtwmrc
	$(RM) $@
	echo '/* ' >>$@
	echo ' * This file is generated automatically from the default' >>$@
	echo ' * vtwm bindings file system.vtwmrc by the vtwm Imakefile.' >>$@
	echo ' */' >>$@
	echo '' >>$@
	echo 'char *defTwmrc[] = {' >>deftwmrc.c
	sed -e '/^#/d' -e 's/"/\\"/g' -e 's/^/    "/' -e 's/$$/",/' \
		system.vtwmrc >>$@
	echo '    (char *) 0 };' >>$@

lastmake.c:
	$(RM) $@
	echo '/* ' >>$@
	echo ' * This file is generated automatically by the vtwm Imakefile.' >>$@
	echo ' */' >>$@
	echo '' >>$@
	echo 'char *lastmake[] = {' >>lastmake.c
	echo '\t"Platform:  '`uname -s`' Version '`uname -v`' Release '`uname -r`'",' >>$@
	echo '\t"Build:  '`date`'",' >>$@
	echo '};\n' >>$@

